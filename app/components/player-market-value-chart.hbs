<EmberChart
  @type={{this.chartType}}
  @data={{this.chartData}}
  @options={{this.chartOptions}}
  @width={{this.chartWidth}}
  @height={{this.chartHeight}}
  @plugins={{this.chartPlugins}}
  @customLegendElement={{this.customLegendElement}}
  @setupChart={{this.setupChart}}
  @destroyChart={{this.destroyChart}}
/>